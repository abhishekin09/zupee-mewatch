{
  "name": "zupee-memwatch",
  "version": "2.2.3",
  "description": "Memory leak detection and monitoring tool for Node.js microservices",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "leak-detector": "dist/cli/leak-detector.js",
    "memwatch-snapshot": "dist/cli/leak-detector.js",
    "zupee-memwatch": "dist/cli/leak-detector.js"
  },
  "files": [
    "dist/**/*",
    "docs/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "start:dashboard": "node dist/dashboard/server.js",
    "start:frontend": "cd dashboard-ui && npm start",
    "test": "node test/test-service.js",
    "test:leak": "node test/test-service.js --simulate-leak",
    "test:scenarios": "node dist/cli/leak-detector.js test",
    "test:snapshot": "node dist/cli/leak-detector.js snapshot",
    "setup": "./scripts/setup.sh",
    "clean": "rm -rf dist node_modules"
  },
  "keywords": [
    "memory",
    "leak",
    "detection",
    "monitoring",
    "nodejs",
    "microservices",
    "zupee"
  ],
  "author": "Zupee Engineering",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/abhishekin09/zupee-mewatch.git"
  },
  "bugs": {
    "url": "https://github.com/abhishekin09/zupee-mewatch/issues"
  },
  "homepage": "https://github.com/abhishekin09/zupee-mewatch#readme",
  "dependencies": {
    "@memlab/api": "^1.0.41",
    "@memlab/core": "^1.1.42",
    "@memlab/heap-analysis": "^1.0.39",
    "commander": "^11.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
